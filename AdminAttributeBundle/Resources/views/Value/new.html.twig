{% extends 'AdminCoreBundle::layout.html.twig' %}

{% block content_title %}
    {{ 'Attribute Value Zone'|trans }}
{% endblock content_title %}

{% block breadcrumb %}

    {% include "@AdminCore/Common/breadcrumb.html.twig" with {
        breadcrumb: {
            'Attributes': {
                'url': url('admin_attribute_list')
            },
            ('#' ~ id): {
                'url': url('admin_attribute_view', { 'id': id })
            },
            "New Value": {
                "active" : true
            }
        }
    } %}

{% endblock breadcrumb %}

{% block content %}

    <div class="row">
        <div class="col-md-8">
            {{ render(url("admin_value_new_component", {id: id})) }}
        </div>
    </div>

{% endblock content %}
