<div class="gallery-cont">

    <ul class="thumbnail-group">
    {% for image in images %}
        {% set imageResize = image|resize({'height': 150, 'width': 150, 'type': 3}) %}
        {% set imageView = image|viewImage %}

        <li>
            <a href="{{ imageView }}" class="thumbnail" data-tc-modules="modal">
                <img src="{{ imageResize }}" width="150" />
            </a>
            <ul class="thumbnail-actions">
                <li>
                    <a href="{{ url("admin_image_delete", {id: image.id}) }}" class="icon-trash-o"></a>
                </li>
            </ul>
        </li>
    {% endfor %}
    </ul>
</div>
